<span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="nb">xrange</span><span class="p">(</span><span class="nb">len</span><span class="p">(</span><span class="n">ttnpt</span><span class="p">)</span><span class="o">-</span><span class="mi">5</span><span class="p">):</span>
    <span class="n">T1</span><span class="o">=</span><span class="n">intdhbcc</span><span class="p">[</span><span class="n">i</span><span class="p">,</span><span class="mi">0</span><span class="p">]</span>
    <span class="n">Gbcc</span><span class="p">[</span><span class="n">i</span><span class="p">,</span><span class="mi">0</span><span class="p">]</span><span class="o">=</span><span class="n">T1</span>
    <span class="n">Gbcc</span><span class="p">[</span><span class="n">i</span><span class="p">,</span><span class="mi">1</span><span class="p">]</span><span class="o">=</span><span class="n">intdhbcc</span><span class="p">[</span><span class="n">i</span><span class="p">,</span><span class="mi">1</span><span class="p">]</span><span class="o">+</span><span class="n">U0bcc</span><span class="o">+</span><span class="p">(</span><span class="n">T1</span><span class="o">/</span><span class="n">T0</span><span class="p">)</span><span class="o">*</span><span class="p">(</span><span class="n">GbccT0</span><span class="o">-</span><span class="n">U0bcc</span><span class="p">)</span><span class="o">-</span><span class="n">nfreebcc</span><span class="o">*</span><span class="n">kbev</span><span class="o">*</span><span class="n">T1</span><span class="o">*</span><span class="n">np</span><span class="o">.</span><span class="n">log</span><span class="p">(</span><span class="n">T1</span><span class="o">/</span><span class="n">T0</span><span class="p">)</span>
    <span class="n">Gbcc</span><span class="p">[</span><span class="n">i</span><span class="p">,</span><span class="mi">2</span><span class="p">]</span><span class="o">=</span><span class="n">intdhbcc</span><span class="p">[</span><span class="n">i</span><span class="p">,</span><span class="mi">2</span><span class="p">]</span><span class="o">+</span><span class="p">(</span><span class="n">T1</span><span class="o">/</span><span class="n">T0</span><span class="p">)</span><span class="o">*</span><span class="n">GbccT0error</span>
